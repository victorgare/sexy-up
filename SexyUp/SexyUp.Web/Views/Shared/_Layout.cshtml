<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Sexy Up!</title>

    @Styles.Render("~/vendor/bootstrap/css")
    @Styles.Render("~/vendor/font-awesome/css")
    @Styles.Render("~/vendor/bootstrap-select/css")
    @Styles.Render("~/vendor/nouislider/css")
    @Styles.Render("~/content/custom-font-icons/css")
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,700">
    @Styles.Render("~/vendor/owl-carousel/css")
    @Styles.Render("~/content/theme/css")

    @Scripts.Render("~/scripts/modernizr/js")

</head>
<body>
    @Html.Partial("_Navigation")
    <div>
        <hr class="invisible" />
        <div class="col-lg-12">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        @RenderBody()
                    </div>
                </div>
            </div>
        </div>
    </div>

    @Scripts.Render("~/vendor/jquery/js")
    @Scripts.Render("~/vendor/popper/js")
    @Scripts.Render("~/vendor/bootstrap/js")
    @Scripts.Render("~/vendor/jquery-cookie/js")
    @Scripts.Render("~/vendor/owl-carousel/js")
    @Scripts.Render("~/vendor/bootstrap-select/js")
    @Scripts.Render("~/vendor/nouislider/js")
    @Scripts.Render("~/vendor/jquery-countdown/js")
    @Scripts.Render("~/scripts/main/js")
    @RenderSection("scripts", false)
</body>
</html>
